<Window x:Class="WpfApp4.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp4"
        mc:Ignorable="d"
        Title="ООО Что-то" Height="450" MinHeight="400" MinWidth="600" Width="800" Icon="./Resources/school_logo.ico">
    <Grid Background="Yellow">
        <Grid.RowDefinitions>
            <RowDefinition Height="70"></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>

        <Image Grid.Row="0" Source="./Resources/school_logo.png"></Image>

        <Frame Grid.Row="1" x:Name="frameNavigate" NavigationUIVisibility="Hidden"></Frame>
    </Grid>
</Window>
